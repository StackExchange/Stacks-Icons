{
  "name": "@stackoverflow/stacks-icons",
  "description": "The icon library for Stack Overflow, Stack Overflow Careers, and the Stack Exchange Network.",
  "version": "4.0.0",
  "repository": "https://github.com/StackExchange/Stacks-Icons",
  "license": "MIT",
  "type": "module",
  "main": "build/index.umd.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/index.umd.js",
      "default": "./build/index.esm.js"
    },
    "./icons": "./build/icons.js",
    "./spots": "./build/spots.js",
    "./src/Icon/*": "./src/Icon/*",
    "./src/Spot/*": "./src/Spot/*"
  },
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "scripts": {
    "start": "npm run build",
    "build": "ts-node ./scripts/build.ts",
    "build:nuget": "cd dotnet && dotnet pack",
    "prepublishOnly": "npm start",
    "publish:nuget": "echo 'Not implemented'",
    "test:nuget": "dotnet test .\\dotnet\\test",
    "version": "ts-node ./scripts/bumpVersions.ts"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "axios": "^1.2.1",
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "concat": "^1.0.3",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "mini-svg-data-uri": "^1.4.4",
    "rollup": "^3.7.3",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
